pipeline {
  agent any
  environment{
    DOCKER=credentials('credential-user-password')
    //DOCKER  USU:PASS
    //DOCKER_USR   USER
    //DOCKER_PSW    PASSSWORD
  }
  stages {
    stage('Docker Build') {
      steps {
        echo 'User Name "${env.DOCKER_USR}"'
        echo 'Pasword Name "${env.DOCKER_PSW}"'
      }
    }
  }
}
